{
  "version": "0.0.1",
  "name": "nectar",
  "main": "src/index.ts",
  "scripts": {
    "start": "tsc",
    "poststart": "npm run copy-stubs",
    "build": "npm run start",
    "prewatch": "npm run copy-stubs",
    "watch": "tsc --watch",
    "test": "jest",
    "test-watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "schemas-to-types": "ts-node ./schemasToTypes.ts",
    "copy-stubs": "mkdir -p ./build/src/testStubs/ && cp -R ./src/testStubs/. ./build/src/testStubs/"
  },
  "dependencies": {
    "joi": "^17.6",
    "joi-to-typescript": "^4.0.5",
    "mailchimp-api-v3": "^1.15.0"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
